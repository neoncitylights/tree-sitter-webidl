{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "webidl",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "includes_statement"
        }
      ]
    },
    "includes_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "includer",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "includes"
        },
        {
          "type": "FIELD",
          "name": "includee",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "const_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "const"
        },
        {
          "type": "FIELD",
          "name": "type",
          "content": {
            "type": "SYMBOL",
            "name": "const_type"
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "const_value"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "const_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "primitive_type"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "const_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "boolean_literal"
        },
        {
          "type": "SYMBOL",
          "name": "float_literal"
        },
        {
          "type": "SYMBOL",
          "name": "integer_literal"
        }
      ]
    },
    "integer_literal": {
      "type": "SYMBOL",
      "name": "_integer"
    },
    "boolean_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "float_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_decimal"
        },
        {
          "type": "STRING",
          "value": "-Infinity"
        },
        {
          "type": "STRING",
          "value": "Infinity"
        },
        {
          "type": "STRING",
          "value": "NaN"
        }
      ]
    },
    "type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "single_type"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "union_type"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "?"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      ]
    },
    "single_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "distinguishable_type"
        },
        {
          "type": "STRING",
          "value": "any"
        }
      ]
    },
    "union_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "union_or_expression"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "union_or_expression": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "left",
            "content": {
              "type": "SYMBOL",
              "name": "union_member_type"
            }
          },
          {
            "type": "STRING",
            "value": "or"
          },
          {
            "type": "FIELD",
            "name": "right",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "union_or_expression"
                },
                {
                  "type": "SYMBOL",
                  "name": "union_member_type"
                }
              ]
            }
          }
        ]
      }
    },
    "union_member_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "distinguishable_type"
        },
        {
          "type": "SYMBOL",
          "name": "union_type"
        }
      ]
    },
    "distinguishable_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "primitive_type"
            },
            {
              "type": "SYMBOL",
              "name": "string_type"
            },
            {
              "type": "SYMBOL",
              "name": "identifier"
            },
            {
              "type": "STRING",
              "value": "object"
            },
            {
              "type": "STRING",
              "value": "symbol"
            },
            {
              "type": "SYMBOL",
              "name": "buffer_related_type"
            },
            {
              "type": "STRING",
              "value": "undefined"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "?"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "buffer_related_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "ArrayBuffer"
        },
        {
          "type": "STRING",
          "value": "SharedArrayBuffer"
        },
        {
          "type": "STRING",
          "value": "DataView"
        },
        {
          "type": "STRING",
          "value": "Int8Array"
        },
        {
          "type": "STRING",
          "value": "Int16Array"
        },
        {
          "type": "STRING",
          "value": "Int32Array"
        },
        {
          "type": "STRING",
          "value": "Uint8Array"
        },
        {
          "type": "STRING",
          "value": "Uint16Array"
        },
        {
          "type": "STRING",
          "value": "Uint32Array"
        },
        {
          "type": "STRING",
          "value": "Uint8ClampedArray"
        },
        {
          "type": "STRING",
          "value": "BigInt64Array"
        },
        {
          "type": "STRING",
          "value": "BigUint64Array"
        },
        {
          "type": "STRING",
          "value": "Float16Array"
        },
        {
          "type": "STRING",
          "value": "Float32Array"
        },
        {
          "type": "STRING",
          "value": "Float64Array"
        }
      ]
    },
    "primitive_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer_type"
        },
        {
          "type": "SYMBOL",
          "name": "float_type"
        },
        {
          "type": "STRING",
          "value": "boolean"
        },
        {
          "type": "STRING",
          "value": "byte"
        },
        {
          "type": "STRING",
          "value": "octet"
        },
        {
          "type": "STRING",
          "value": "bigint"
        }
      ]
    },
    "integer_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "unsigned"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "short"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "long"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "long"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    "float_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "restricted"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "float"
            },
            {
              "type": "STRING",
              "value": "double"
            }
          ]
        }
      ]
    },
    "string_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "ByteString"
        },
        {
          "type": "STRING",
          "value": "DOMString"
        },
        {
          "type": "STRING",
          "value": "USVString"
        }
      ]
    },
    "identifier_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_integer": {
      "type": "PATTERN",
      "value": "-?([1-9][0-9]*|0[Xx][0-9A-Fa-f]+|0[0-7]*)"
    },
    "_decimal": {
      "type": "PATTERN",
      "value": "-?(([0-9]+\\.[0-9]*|[0-9]*\\.[0-9]+)([Ee][+-]?[0-9]+)?|[0-9]+[Ee][+-]?[0-9]+)"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[_-]?[A-Za-z][0-9A-Z_a-z-]*"
    },
    "string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "other": {
      "type": "PATTERN",
      "value": "[^\\t\\n\\r 0-9A-Za-z]"
    },
    "_whitespace": {
      "type": "PATTERN",
      "value": "[\\t\\n\\r ]+"
    },
    "comment": {
      "type": "PATTERN",
      "value": "\\/\\/.*|\\/\\*(.|\\n)*?\\*\\/"
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "_whitespace"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}
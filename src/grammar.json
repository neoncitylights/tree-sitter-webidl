{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "webidl",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": []
    },
    "identifier_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "integer": {
      "type": "PATTERN",
      "value": "-?([1-9][0-9]*|0[Xx][0-9A-Fa-f]+|0[0-7]*)"
    },
    "decimal": {
      "type": "PATTERN",
      "value": "-?(([0-9]+\\.[0-9]*|[0-9]*\\.[0-9]+)([Ee][+-]?[0-9]+)?|[0-9]+[Ee][+-]?[0-9]+)"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[_-]?[A-Za-z][0-9A-Z_a-z-]*"
    },
    "string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "other": {
      "type": "PATTERN",
      "value": "[^\\t\\n\\r 0-9A-Za-z]"
    },
    "whitespace": {
      "type": "PATTERN",
      "value": "[\\t\\n\\r ]+"
    },
    "comment": {
      "type": "PATTERN",
      "value": "\\/\\/.*|\\/\\*(.|\\n)*?\\*\\/"
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "whitespace"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}